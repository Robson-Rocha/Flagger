{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-flagger",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/lib/Flagger.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-setandreset",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/samples/setandreset/Flagger.Samples.SetAndReset.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-setandunset",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/samples/setandunset/Flagger.Samples.SetAndUnset.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-setandunsetifunset",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/samples/setandunsetifunset/Flagger.Samples.SetAndUnsetIfUnset.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-toggle",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/samples/toggle/Flagger.Samples.Toggle.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}