    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flagger.Samples.SetAndReset",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-setandreset",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/samples/setandreset/bin/Debug/netcoreapp2.2/Flagger.Samples.SetAndReset.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/samples/setandreset",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Flagger.Samples.SetAndUnset",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-setandunset",
            "program": "${workspaceFolder}/src/samples/setandunset/bin/Debug/netcoreapp2.2/Flagger.Samples.SetAndUnset.dll",
            "args": [],
